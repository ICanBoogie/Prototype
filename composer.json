{
  "name": "icanboogie/prototype",
  "type": "library",
  "description": "Implements the Prototype design pattern; getters/setters",
  "keywords": [
    "prototype",
    "getter",
    "setter",
    "facade"
  ],
  "homepage": "https://icanboogie.org/",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Olivier Laviale",
      "email": "olivier.laviale@gmail.com",
      "homepage": "https://olvlvl.com/",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/ICanBoogie/Prototype/issues",
    "source": "https://github.com/ICanBoogie/Prototype"
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=8.1",
    "ext-json": "*",
    "icanboogie/accessor": "^4.0"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.10",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "ICanBoogie\\": "lib",
      "ICanBoogie\\Prototype\\": "lib/Prototype"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/cases.php"
    ],
    "psr-4": {
      "Test\\ICanBoogie\\": "tests"
    }
  }
}
