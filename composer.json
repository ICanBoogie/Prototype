{
	"name": "icanboogie/prototype",
	"type": "library",
	"description": "Implements the Prototype design pattern; getters/setters",
	"keywords": [
		"prototype",
		"getter",
		"setter",
		"facade"
	],
	"homepage": "https://icanboogie.org/",
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Olivier Laviale",
			"email": "olivier.laviale@gmail.com",
			"homepage": "https://olvlvl.com/",
			"role": "Developer"
		}
	],
	"support": {
		"issues": "https://github.com/ICanBoogie/Prototype/issues",
		"source": "https://github.com/ICanBoogie/Prototype"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"require": {
		"php": ">=7.2",
		"ext-json": "*",
		"icanboogie/accessor": "^3.0",
		"icanboogie/common": "^1.2"
	},
	"require-dev": {
		"phpstan/phpstan": "^0.12.63",
		"phpunit/phpunit": "^8.5"
	},
	"autoload": {
		"psr-4": {
			"ICanBoogie\\": "lib",
			"ICanBoogie\\Prototype\\": "lib/Prototype"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"ICanBoogie\\": "tests"
		}
	},
	"config": {
		"sort-packages": true
	}
}
