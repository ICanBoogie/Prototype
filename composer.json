{
	"name": "icanboogie/prototype",
	"type": "library",
	"description": "Implements the Prototype design pattern; getters/setters",
	"keywords": [ "prototype", "getter", "setter", "facade" ],
	"homepage": "http://icanboogie.org/",
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Olivier Laviale",
			"email": "olivier.laviale@gmail.com",
			"homepage": "http://www.weirdog.com/",
			"role" : "Developer"
		}
	],
	"support": {
		"issues": "https://github.com/ICanBoogie/Prototype/issues",
		"source": "https://github.com/ICanBoogie/Prototype"
	},

	"require": {
		"php": ">=5.5.0",
		"icanboogie/common": "^1.2.2",
		"icanboogie/accessor": "^1.0.1"
	},

	"autoload": {
		"psr-4": {
			"ICanBoogie\\Object\\": "lib/Object",
			"ICanBoogie\\Prototype\\": "lib/Prototype"
		},
		"classmap": [ "lib/Object.php", "lib/Prototype.php", "lib/PrototypeTrait.php" ]
	},

	"extra": {
		"icanboogie": {
			"config-constructor": {
				"prototypes": "ICanBoogie\\Prototype\\Hooks::synthesize_config#hooks"
			}
		}
	}
}
